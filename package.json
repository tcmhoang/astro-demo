{
  "name": "astro-demo",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/solid-js": "^3.0.1",
    "@astrojs/svelte": "^4.0.2",
    "@nanostores/solid": "^0.4.2",
    "astro": "^3.0.12",
    "nanostores": "^0.9.3",
    "solid-js": "^1.7.11",
    "svelte": "^4.2.0"
  },
  "devDependencies": {
    "bun-types": "^1.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0"
  },
  "prettier": {
    "astroAllowShorthand": true,
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ],
    "pluginSearchDirs": [
      "."
    ],
    "plugins": [
      "prettier-plugin-astro"
    ]
  }
}
